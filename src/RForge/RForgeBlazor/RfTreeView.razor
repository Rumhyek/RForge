
<aside class="menu rf-treeview @CssClass">

    <CascadingValue Value=Context>
        @if (ShowAsPrerender == true)
        {
            @PrerenderNodes
        }
        else
        {
            if (Nodes != null)
            {
                @Nodes
            }
            else
            {
                @ChildContent
            }
        }
    </CascadingValue>

</aside>
